<h1>Sign Up</h1>
<%= form_with url: users_path, model: User do |f| %>
<div>
	<%= f.label :full_name %>
	<%= f.text_field :full_name %>
</div>

<div>
	<%= f.label :email %>
	<%= f.email_field :email %>
</div>

<div>
	<%= f.label :team_name %>
	<%= f.text_field :team_name %>
</div>

<div>	
	<%= f.label :password %>
	<%= f.password_field :password %>
</div>

<div>
	<%= f.submit "Sign Up" %>
</div>
<% end %>
